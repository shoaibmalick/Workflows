    {
      "parameters": {
        "jsCode": "// Basic input validation & sanitization\nconst unsafePattern = /<\\/?script.*?>/gi;\n\nfunction sanitize(value) {\n  if (typeof value !== 'string') return value;\n  return value.replace(unsafePattern, '').trim();\n}\n\nconst items = $input.all(); \n\n// Process each item \nfor (const item of items) \n{ \n  const json = item.json; \n  // Email format validation (basic) \n  const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/; \n\n  json.email_validation = 'passed';\n  if (!emailRegex.test(json.lead_email)) \n  { \n    json.email_validation = 'failed';  \n  }\n  \n  // Sanitize all string fields \n  \n  for (const key of Object.keys(json)) \n  { \n    json[key] = sanitize(json[key]); \n  }\n\n  // Mark lead as validated \n  json.validation_status = 'passed'; \n  } \n\n  return items;\n"
      },
